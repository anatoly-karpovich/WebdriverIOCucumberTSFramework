{
  "name": "wdio6_typescript_bdd",
  "version": "1.0.0",
  "description": "WebdriverIO with TypeScript, BDD framework to test Automation practise website",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio wdio.conf.js",
    "report": "allure generate --clean allure-results && allure open",
    "installChrome": "npm install chromedriver --chromedriver_filepath=./drivers/chromedriver_win32.zip",
    "startFirefox": "cd drivers && geckodriver.exe --binary \"C:/Program Files/Mozilla Firefox/firefox.exe\" --port 8080"
  },
  "author": "MD SADAB SAQIB",
  "license": "ISC",
  "dependencies": {
    "@wdio/cli": "^7.1.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@wdio/allure-reporter": "^7.1.1",
    "@wdio/cucumber-framework": "^7.1.2",
    "@wdio/junit-reporter": "^7.1.2",
    "@wdio/local-runner": "^7.1.2",
    "@wdio/sync": "^7.1.1",
    "allure-commandline": "^2.13.8",
    "chai": "^4.3.4",
    "chromedriver": "^89.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "wdio-chromedriver-service": "^7.0.0",
    "webdriverio": "^7.1.1"
  }
}
